# Example .env for Event Tracking System

# UX event sink configuration
VITE_UX_EVENT_SINK_TYPE=openobserve
VITE_UX_EVENT_SINK_URL=https://your-openobserve-instance/api/events
VITE_UX_EVENT_SINK_API_KEY=your-ux-api-key

# Simulation event sink configuration
VITE_SIM_EVENT_SINK_TYPE=openobserve
VITE_SIM_EVENT_SINK_URL=https://your-openobserve-instance/api/sim-events
VITE_SIM_EVENT_SINK_API_KEY=your-sim-api-key
